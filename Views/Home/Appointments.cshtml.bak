
<div class="scrollable container-fluid" ng-controller="AppointmentsController">
    <div class=row>
        <div class=col-sm-12>
            <div class="page-headers cus-panels-with-pad">
                <div class="block-options pull-right">
                    <button type=button class=sec-cus-buttons ng-click="clearAppointment()"  data-toggle=modal data-target="#myModal"> <i class="fa fa-plus"></i> New Appointment </button>
                    <!-- Modal -->
                    <div class="modal fade" id="myModal" tabindex=-1 role=dialog aria-labelledby=myModalLabel>
                        <div class=modal-dialog role=document>
                            <div class=modal-content>
                                <div class="modal-header cus-bg ng-scope">
                                    <button type=button class=close data-dismiss=modal aria-label=Close> <span aria-hidden=true>×</span> <span class=sr-only>Close</span> </button>
                                    <h4 class="modal-title ng-binding">Add New Appointment</h4>
                                </div>
                                <div class=modal-body>
                                    <form name="appointmentForm">
                                        <div class=row>
                                            <div class="col-md-6 col-lg-6">
                                                <div class=form-group>
                                                    <label for="">Customer Name<span class=required> *</span></label>
                                                    <div class="control-group" ng-class="{true: 'error'}[submitted && appointmentForm.custnam.$invalid]">
                                                        <select class=form-control id="addappointmentID" name="custnam" ng-model="custnam" ng-change="pets(custnam)" ng-options="custorids.customer_id as custorids.first_name for custorids in customrids"  required>
                                                        <option value="" disabled selected style='display:none;'>-- Select Customer Name --</option></select>
                                                        <span style="color:red" ng-show="submitted && appointmentForm.custnam.$error.required">Select Reminder is required.</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6 col-lg-6">
                                                <div class=form-group>
                                                    <label for="">Pet Name<span class="required">*</span></label>
                                                    <div class="control-group" ng-class="{true: 'error'}[submitted && appointmentForm.petnm.$invalid]">
                                                        <select class=form-control ng-model="petnm" name="petnm" ng-options="petcusters.pet_id as petcusters.pet_name for petcusters in petcustomers" required>
                                                        <option value="" disabled selected style='display:none;'>-- Select Pet Name --</option></select>
                                                        <span style="color:red" ng-show="submitted && appointmentForm.petnm.$error.required">Pet Name is required.</span>
                                                    </div>
                                                    </div>
                                            </div>
                                        </div>
                                        <div class="select-groom">
                                            <div class=row>
                                                <div class="col-md-6 col-lg-6">
                                                    <div class=form-group>
                                                        <label for="">Service <span class=required> *</span></label>
                                                        <div class="control-group" ng-class="{true: 'error'}[submitted && appointmentForm.servetype.$invalid]">
                                                            <select class=form-control id="addappointmentID" ng-disabled="servicenamevalue"  ng-model="servetype" ng-change="modify()" name="servetype" ng-options="service as service.ld_name for service in svctype" required>
                                                                <option value="" disabled selected style='display:none;'>-- Select Servicee --</option>
                                                            </select>
                                                            <span style="color:red" ng-show="submitted && appointmentForm.servetype.$error.required">Service is required.</span>
                                                        </div>
                                                         </div>
                                                </div>
                                                <div class="col-md-6 col-lg-6">
                                                    <div class=form-group>
                                                        <label for="">Service Type<span class="required">*</span></label>
                                                        <div class="control-group" ng-class="{true: 'error'}[submitted && appointmentForm.statustype1.$invalid]">
                                                            <select class=form-control ng-model="statustype1" ng-disabled="serviceTypeName" name="statustype1" ng-options="stype.service_id as stype.service_name for stype in sertype" required>
                                                            <option value="" disabled selected style='display:none;'>-- Select Service Type --</option></select>
                                                            <span style="color:red" ng-show="submitted && appointmentForm.statustype1.$error.required">Service Type is required.</span>
                                                        </div>
                                                         </div>
                                                </div>
                                            </div>
                                            <div class="row boarding" ng-show="serviceText != 'Grooming'">
                                                <div class="col-md-6 col-lg-6">
                                                    <div class=form-group>
                                                        <label for="">Start Date<span class="required">*</span></label>
                                                        <div class="control-group" ng-class="{true: 'error'}[submitted && appointmentForm.sttdat.$invalid]">
                                                            <input type="date" class=form-control ng-model="sttdat" name="sttdat" required>
                                                            <span style="color:red" ng-show="submitted && appointmentForm.sttdat.$error.required">Start Date is required.</span>
                                                        </div>
                                                         </div>

                                                </div>
                                                <div class="col-md-6 col-lg-6">
                                                    <div class=form-group>
                                                        <label for="">End Date<span class="required">*</span></label>
                                                        <div class="control-group" ng-class="{true: 'error'}[submitted && appointmentForm.enndt.$invalid]">
                                                            <input type="date" class=form-control ng-model="enndt" name="enndt" required>
                                                            <span style="color:red" ng-show="submitted && appointmentForm.enndt.$error.required">End Date is required.</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-lg-6">
                                                    <div class=form-group>
                                                        <label for="">Chargeable days<span class=required> *</span></label>
                                                        <div class="control-group" ng-class="{true: 'error'}[submitted && appointmentForm.chrgdas.$invalid]">
                                                            <input class=form-control ng-model="chrgdas" name="chrgdas" required>
                                                            <span style="color:red" ng-show="submitted && appointmentForm.chrgdas.$error.required">End Date is required.</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-lg-6">
                                                    <div class="pad-15-0">
                                                        <div>
                                                            <label>
                                                                <input type=checkbox ng-model="bokwhlrun" name="bokwhlrun" ng-true-value="'Y'" ng-false-value="'N'" class=check>Bookwholerun
                                                            </label>
                                                        </div>
                                                        <div>
                                                            <label>
                                                                <input type=checkbox ng-model="remin" name="remin" ng-true-value="'Y'" ng-false-value="'N'" required class="check">Reminder
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row grooming" ng-show="serviceText == 'Grooming'">
                                                <div class="col-md-6 col-lg-6">
                                                    <div class="mar-top15 pad-15-0">
                                                        <div>
                                                            <label>
                                                                <input type=checkbox ng-model="recurr" name="recurr" ng-true-value="'Y'" ng-false-value="'N'" class=check>Recurring
                                                            </label>
                                                        </div>
                                                        
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-lg-6">
                                                    <div class="mar-top15 pad-15-0">
                                                        
                                                        <div>
                                                            <label>
                                                                <input type=checkbox ng-model="remin" class="check" name="remin" ng-true-value="'Y'" ng-false-value="'N'" required>Reminder
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row groom-hide" ng-show="serviceText == 'Grooming'">
                                                <div class="col-md-6 col-lg-6">
                                                    <div class=form-group>
                                                        <label for="">Groomer<span class=required> *</span></label>
                                                        <select class="form-control test12" ng-model="gromeval" name="gromeval" ng-options="grmomer.employee_id as grmomer.first_name for grmomer in groomers"></select>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-lg-6">
                                                    <div class=form-group>
                                                        <label for="">Groom Style</label>
                                                        <select class=form-control ng-model="gromstyl1" name="gromstyl1" ng-options="gromsty.ld_id as gromsty.ld_name for gromsty in groomstyle"></select>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-lg-6">
                                                    <div class=form-group>
                                                        <label for="">Appointment Date</label>
                                                        <input type="date" class=form-control ng-model="appnt" name="appnt" required>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-lg-6">
                                                    <div class=form-group>
                                                        <label for="">Estimated Duration<span class=required> *</span></label>
                                                        <input class=form-control ng-model="estimat" name="estimat" required>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class=row>
                                            <div class="col-md-6 col-lg-6">
                                                <div class=mar-10-0>
                                                    <div>
                                                        <label>
                                                            <input type=checkbox ng-model="pick" name="pick" class=check ng-true-value="'Y'" ng-false-value="'N'">Pickup
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6 col-lg-6">
                                                <div class=mar-10-0>
                                                    <div>
                                                        <label>
                                                            <input type=checkbox ng-model="delvry" name="delvry" ng-true-value="'Y'" ng-false-value="'N'" class=check>Delivery
                                                        </label>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <div class=row>
                                            <div class="col-md-6 col-lg-6">
                                                <div class=form-group>
                                                    <label for="">Pickup Date</label>
                                                    <input type="date" class=form-control ng-model="pickupdat" name="pickupdat" required>
                                                </div>

                                            </div>
                                            <div class="col-md-6 col-lg-6">
                                                <div class=form-group>
                                                    <label for="">Delivery Date</label>
                                                    <input type="date" class=form-control ng-model="dedate" name="dedate" required>
                                                </div>
                                            </div>
                                        </div>
                                        <div class=row>
                                            <div class="col-md-12 col-lg-12">
                                                <div class=form-group>
                                                    <label for="">Status<span class=required> *</span></label>
                                                    <div class="control-group" ng-class="{true: 'error'}[submitted && appointmentForm.stus1.$invalid]">
                                                        <select class=form-control ng-model="stus1" name="stus1" ng-options="stusval.ld_id as stusval.ld_name for stusval in statusval" required><option value="" disabled selected style='display:none;'>-- Select Status --</option></select>
                                                        <span style="color:red" ng-show="submitted && appointmentForm.stus1.$error.required">Status is required.</span>
                                                    </div>
                                                 </div>
                                            </div>
                                        </div>
                                        <div class=row>
                                            <div class="col-md-12 col-lg-12">
                                                <div class=form-group>
                                                    <label for="">Notes</label>
                                                    <textarea class=form-control ng-model="adnotes" name="adnotes" rows=3></textarea>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <div class=modal-footer>
                                    <button type=button class="btn btn-default" data-dismiss=modal>Close</button>
                                    <button type="button" class="btn btn-primary" ng-click="showconfirmbox();submitted=true;newAppoint(servetype)" data-dismiss="modal">Add</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <h3>Manage Appointment List</h3>
            </div>
