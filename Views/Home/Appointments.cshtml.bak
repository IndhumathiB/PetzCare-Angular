                                        <div class="col-xs-3 col-sm-3 single-w-lh margin-t-b text-center"> <span>{{brmdata.service_type_name}} </span></div>
                                        <div class="col-xs-3 col-sm-3 single-w-lh margin-t-b text-center"><span>{{brmdata.appointment_date}} </span></div>
