            <div class="modal fade" id="appointmentdetail" tabindex=-1 role=dialog aria-labelledby=myModalLabel>
                <div class=modal-dialog role=document>
                    <div class=modal-content>
                        <div class="modal-header cus-bg ng-scope">
                            <button type=button class=close data-dismiss=modal aria-label=Close> <span aria-hidden=true>×</span> <span class=sr-only>Close</span> </button>
                            <h4 class="modal-title ng-binding">Appointment Details</h4>
                        </div>
                        <div class=modal-body>
                            <form>
                               
                                <div class=row>
                                    <div class="col-md-6 col-lg-6">
                                        <div class=form-group>
                                            <label for="">Customer Name<span class=required> *</span></label>
                                            <input class=form-control ng-model="appointment.customer_name" name="cname" required>
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6">
                                        <div class=form-group>
                                            <label for="">Service Name</label>
                                            <input class=form-control ng-model="appointment.service_name" name="pname">
                                        </div>
                                    </div>
                                </div>
                                
                                
                              

                                <fieldset>
                                    <legend>Service List</legend>
                                    <div class=row>
                                        <div class="col-xs-3 col-sm-3 single-w-lh text-center"> <strong>Service</strong> </div>
                                        <div class="col-xs-3 col-sm-3 single-w-lh text-center"> <strong>Service Type</strong> </div>
                                        <div class="col-xs-3 col-sm-3 single-w-lh text-center"> <strong>Date</strong> </div>
                                        <div class="col-xs-3 col-sm-3 single-w-lh text-center"> <strong>Action</strong> </div>
                                    </div>
                                    <div ng-show="sername == 'Boarding'" class=row ng-repeat="brmdata in brdappsedetails">
