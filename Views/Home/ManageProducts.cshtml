                            <div id="editproductdloader" class="overlays" style="height:100%; width:97%;" ng-show="Products">
                                <img src=" ~/images/spinningcurser.gif" style="position:absolute; height:30px;  left:47%; top:40%; width:30px;" />
                            </div>
                            <div class="table-responsive mar-top15">
                                <table id=tbl_profile class="table table-borderless table-striped">
                                  
                                    <tbody>
                                        <tr><td><label>Product Image</label>
                                                <img ng-src="data:image/JPEG;base64,{{productValues.file_data}}" class="img-circle" style="width:120px;height:90px;" alt="User image" ng-show="productValues.file_data.length != 0">
                                                <img src=~/images/default_product.jpg alt=avatar style="width:50px" class=img-circle ng-show="productValues.file_data.length == 0">
                                            </td></tr>
                                        <tr>
                                            <td>
                                                <label>Product Code</label>
                                            <td><a id=a_name>{{productValues.product_code}}</a>
                                        <tr>
                                            <td>
                                                <label>Product Name</label>
                                            <td><a id=a_name>{{productValues.product_name}}</a>
                                        <tr>
                                            <td>
                                                <label>Product Description</label>
                                            <td> <span id=u_name>{{productValues.product_desc}}</span>
                                        <tr>
                                            <td>
                                                <label>Quantity</label>
                                            <td> <span>{{productValues.quantity}}</span>
                                        <tr>
                                            <td>
                                                <label>Reorder Quantity</label>
                                            <td><a id=a_name>{{productValues.reorder_quantity}}</a>

                                        <tr>
                                            <td>
                                                <label>Vendor ID</label>
                                            <td> <span>{{productValues.vendor_id}}</span>
                                        <tr>
                                            <td>
                                                <label>Vendor Name</label>
                                            <td><a id=a_name>{{productValues.vendor_name}}</a>
                                         
                                        <tr>
                                            <td>
                                                <label>Notes</label>
                                            <td><a id=a_name>{{productValues.notes}}</a>
                                        
                                       

                      </table>
                            </div>
                        </div>
                        <div class=modal-footer>
                            <button type=button class="btn btn-default" data-dismiss=modal>Close</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- .content End -->
        </div>
    </div>
</div>
